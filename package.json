{
  "name": "tic-tac-toe-js",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "lodash": "^4.17.21",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-lodash": "^0.1.2",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "npm:install:global": "npm install -g yarn standard serve",
    "setup": "npm run npm:install:global",
    "yarn:licenses:list:prod": "yarn licenses list --prod > LICENSES.txt",
    "yarn:licenses:generate-disclaimer:prod": "yarn licenses generate-disclaimer --prod > LICENSES.md",
    "yarn:licenses:prod": "npm run yarn:licenses:list:prod && npm run yarn:licenses:generate-disclaimer:prod",
    "LICENSES": "npm run yarn:licenses:prod",
    "standard": "standard --fix src/**/*.js*",
    "lint:js": "npm run standard",
    "lint": "npm run lint:js",
    "react:test": "react-scripts test",
    "react:start": "react-scripts start",
    "react:build": "react-scripts build",
    "react:eject": "react-scripts eject",
    "prestart": "npm run lint",
    "start": "npm run react:start",
    "prebuild": "npm run lint",
    "build": "npm run react:build",
    "serve:build": "serve -s build",
    "test": "npm run react:test",
    "eject": "npm run react:eject"
  },
  "standard": {
    "global": [
      "alert"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0"
  }
}
